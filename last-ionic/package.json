{
  "name": "last-ionic",
  "description": "An Ionic drullan.dev project",
  "private": true,
  "version": "0.0.3",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test.e2e": "cypress run",
    "test.unit": "vitest",
    "lint": "eslint",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,md}\"",
    "precommit": "npm run format && lint-staged",
    "prepare": "husky install",
    "build-electron": "tsc -p tsconfig.electron.json",
    "electron": "npm run build-electron && electron ./dist/electron/main.js",
    "electron-build": "npm run build-electron && electron-builder"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "@awesome-cordova-plugins/core": "^6.8.0",
    "@capacitor/action-sheet": "6.0.2",
    "@capacitor/android": "6.1.2",
    "@capacitor/cli": "^6.1.2",
    "@capacitor/core": "^6.1.2",
    "@capacitor/geolocation": "6.0.1",
    "@capacitor/ios": "6.1.2",
    "@capacitor/preferences": "^6.0.2",
    "@hookform/resolvers": "^3.9.0",
    "@ionic/core": "^8.3.0",
    "@ionic/react": "^8.3.0",
    "@ionic/react-router": "^8.3.0",
    "@tanstack/react-query": "^5.54.1",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "@types/sqlite3": "^3.1.11",
    "axios": "^1.7.7",
    "crypto-browserify": "^3.12.0",
    "crypto-js": "^4.2.0",
    "dompurify": "^3.1.6",
    "dotenv": "^16.4.5",
    "eslint-config-react-app": "^7.0.1",
    "framer-motion": "^11.5.4",
    "i18next": "^23.14.0",
    "i18next-browser-languagedetector": "^8.0.0",
    "ionicons": "^7.4.0",
    "lodash": "^4.17.21",
    "mapbox-gl": "^3.6.0",
    "node-cache": "^5.1.2",
    "node-gyp": "^10.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-ga": "^3.3.1",
    "react-google-recaptcha": "^3.1.0",
    "react-hook-form": "^7.53.0",
    "react-i18next": "^15.0.1",
    "react-lazy-load-image-component": "^1.6.2",
    "react-router": "^5.3.4",
    "react-router-dom": "^5.3.4",
    "reselect": "^5.1.1",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "styled-components": "^6.1.13",
    "swiper": "^11.1.12",
    "winston": "^3.14.2",
    "yup": "^1.4.0",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-transform-runtime": "^7.25.4",
    "@babel/preset-env": "^7.25.4",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@types/crypto-js": "^4.2.2",
    "@types/dompurify": "^3.0.5",
    "@types/electron": "^1.6.10",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.7",
    "@types/node": "^22.5.4",
    "@types/react": "18.3.4",
    "@types/react-dom": "^18.3.0",
    "@types/react-google-recaptcha": "^2.1.9",
    "@types/react-lazy-load-image-component": "^1.6.4",
    "@vitejs/plugin-react": "^4.3.1",
    "cypress": "^13.14.2",
    "depcheck": "^1.4.7",
    "electron": "^32.0.2",
    "electron-builder": "^24.13.3",
    "electron-packager": "^17.1.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.35.2",
    "husky": "9.1.5",
    "jest": "^29.7.0",
    "jsdom": "^25.0.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "sass": "^1.78.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.5.4",
    "vite": "5.4.2",
    "vite-plugin-env-compatible": "^2.0.1",
    "vitest": "^2.0.5",
    "wait-on": "^8.0.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "engines": {
    "node": "20.13.1",
    "npm": ">=10.2.3"
  },
  "build": {
    "appId": "com.example.yourapp",
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "arch": [
        "arm64"
      ]
    }
  }
}
