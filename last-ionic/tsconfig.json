{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "typeRoots": ["./node_modules/@types", "./types", "./global.d.ts"]
  },
  "include": ["src", "assets/data/fonts_list.ts", "src/config/pages.tsx", "src/config/routes.ts", "static", "src/config/firebase.ts", "src/.sheet", "src/reducer/data/global/data.setters.ts", "stuff/hobform"],
  "references": [{ "path": "./tsconfig.node.json" }]
}
